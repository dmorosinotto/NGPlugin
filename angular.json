{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "cli": {
    "packageManager": "pnpm"
  },
  "defaultProject": "web-app",
  "projects": {
    "web-app": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "inlineTemplate": true,
          "inlineStyle": true,
          "skipTests": true
        },
        "@schematics/angular:class": {
          "skipTests": true
        },
        "@schematics/angular:directive": {
          "skipTests": true
        },
        "@schematics/angular:guard": {
          "skipTests": true
        },
        "@schematics/angular:interceptor": {
          "skipTests": true
        },
        "@schematics/angular:pipe": {
          "skipTests": true
        },
        "@schematics/angular:resolver": {
          "skipTests": true
        },
        "@schematics/angular:service": {
          "skipTests": true
        }
      },
      "root": "projects/web-app",
      "sourceRoot": "projects/web-app/src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/web-app",
            "index": "projects/web-app/src/index.html",
            "main": "projects/web-app/src/main.ts",
            "polyfills": "projects/web-app/src/polyfills.ts",
            "tsConfig": "projects/web-app/tsconfig.app.json",
            "assets": [
              "projects/web-app/src/favicon.ico",
              "projects/web-app/src/assets"
            ],
            "styles": [
              "projects/web-app/src/styles.css"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "1mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "4kb"
                }
              ],
              "fileReplacements": [
                {
                  "replace": "projects/web-app/src/environments/environment.ts",
                  "with": "projects/web-app/src/environments/environment.prod.ts"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "buildOptimizer": false,
              "optimization": false,
              "vendorChunk": true,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "browserTarget": "web-app:build:production"
            },
            "development": {
              "browserTarget": "web-app:build:development"
            }
          },
          "defaultConfiguration": "development",
          "options": {
            "proxyConfig": "proxy.conf.json"
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "web-app:build"
          }
        }
      }
    },
    "shared": {
      "projectType": "library",
      "root": "projects/shared",
      "sourceRoot": "projects/shared/src",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:ng-packagr",
          "options": {
            "project": "projects/shared/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "projects/shared/tsconfig.lib.prod.json"
            },
            "development": {
              "tsConfig": "projects/shared/tsconfig.lib.json"
            }
          },
          "defaultConfiguration": "production"
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/shared/src/test.ts",
            "tsConfig": "projects/shared/tsconfig.spec.json",
            "karmaConfig": "projects/shared/karma.conf.js"
          }
        }
      }
    },
    "try-plugin": {
      "projectType": "library",
      "root": "projects/try-plugin",
      "sourceRoot": "projects/try-plugin/src",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:ng-packagr",
          "options": {
            "project": "projects/try-plugin/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "projects/try-plugin/tsconfig.lib.prod.json"
            },
            "development": {
              "tsConfig": "projects/try-plugin/tsconfig.lib.json"
            }
          },
          "defaultConfiguration": "production"
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/try-plugin/src/test.ts",
            "tsConfig": "projects/try-plugin/tsconfig.spec.json",
            "karmaConfig": "projects/try-plugin/karma.conf.js"
          }
        }
      }
    },
    "formly-custom": {
      "projectType": "library",
      "root": "projects/formly-custom",
      "sourceRoot": "projects/formly-custom/src",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:ng-packagr",
          "options": {
            "project": "projects/formly-custom/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "projects/formly-custom/tsconfig.lib.prod.json"
            },
            "development": {
              "tsConfig": "projects/formly-custom/tsconfig.lib.json"
            }
          },
          "defaultConfiguration": "production"
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/formly-custom/src/test.ts",
            "tsConfig": "projects/formly-custom/tsconfig.spec.json",
            "karmaConfig": "projects/formly-custom/karma.conf.js"
          }
        }
      }
    }
  }
}